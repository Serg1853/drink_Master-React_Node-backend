    "/drinks/{id}": {
      "get": {
        "tags": ["Drinks"],
        "summary": "Get cocktail by id",
        "description": "Get cocktails by id",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Get cocktails by id",
            "required": true,
            "schema": {
              "type": "string",
              "examples": ["639b6de9ff77d221f190c520"]
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/GetByIdResponse"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      }
    },
    "/drinks/favorite/add/{id}": {
      "post": {
        "tags": ["Drinks"],
        "summary": "Add cocktail to favorite",
        "description": "Add cocktails to favorite by id",
